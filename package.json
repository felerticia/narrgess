{
  "name": "conduite-center-front",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "firebase": "^6.0.1",
    "html-to-draftjs": "^1.4.0",
    "html2pdf.js": "^0.9.0",
    "lodash": "^4.17.13",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.25",
    "react": "^16.3.2",
    "react-big-calendar": "^0.19.1",
    "react-color": "^2.14.1",
    "react-confirm-alert": "^2.0.2",
    
    "react-csv": "^1.0.14",
    "react-datepicker": "^1.4.1",
    "react-dates": "^16.7.0",
    "react-dom": "^16.3.2",
    "react-draft-wysiwyg": "^1.12.13",
    "react-places-autocomplete": "^7.1.2",
    "react-responsive-modal": "^3.0.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-slick": "^0.23.1",
    "react-time-picker": "^3.5.2",
    "react-times": "^3.0.0",
    "react-with-direction": "^1.1.0",
    "react-youtube": "^7.9.0",
    "react-youtube-background": "^0.1.4",
    "recharts": "^1.1.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "sha256": "^0.2.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js ./src/",
    "lint-fix": "eslint --ext .js ./src/ --fix",
    "docker-build": "yarn docker-build-dev",
    "docker-build-dev": "docker build -f Dockerfile.development -t conduite-center-front-dev .",
    "docker-build-prod": "docker build -f Dockerfile.production -t conduite-center-front-prod .",
    "docker-run": "yarn docker-run-dev",
    "docker-run-dev": "docker-compose -f docker-compose.development.yml up",
    "docker-run-prod": "docker-compose -f docker-compose.production.yml up",
    "docker-stop": "yarn docker-stop-dev",
    "docker-stop-dev": "docker-compose -f docker-compose.development.yml stop",
    "docker-stop-prod": "docker-compose -f docker-compose.production.yml stop",
    "deploy": "docker-machine env conduitecenter-front && eval $(docker-machine env conduitecenter-front) && yarn build && yarn docker-build-prod && yarn docker-run-prod -d && docker-machine env -u && eval $(docker-machine env -u)",
    "deploy-no-build": "docker-machine env conduitecenter-front && eval $(docker-machine env conduitecenter-front) && yarn docker-build-prod && yarn docker-run-prod -d && docker-machine env -u && eval $(docker-machine env -u)"
  },
  "devDependencies": {
    "babel-core": "6",
    "babel-loader": "^7.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4"
  }
}
